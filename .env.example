APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# LOGGING
LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# DATABASE
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

# AUTH
AUTH_METHOD=alma
AUTH_API_ENDPOINT=https://www.google.de
#AUTH_API_ENDPOINT=https://httpbin.org/post
AUTH_API_TIMEOUT=15
AUTH_API_DEBUG=false
AUTH_API_STORAGE_LOG_FILE=curl.log

# TEST ACCOUNTS
ADMIN_USER=admin
ADMIN_PASSWORD=admin
ADMIN_EMAIL=admin@example.org
TEST_USER_01=test1
TEST_USER_01_PASSWORD=test1
TEST_USER_01_EMAIL=test1@example.org
TEST_USER_02=test2
TEST_USER_02_PASSWORD=test2
TEST_USER_02_EMAIL=test2@example.org

# SANCTUM
SANCTUM_STATEFUL_DOMAINS=roomz.localhost
SPA_URL=http://roomz.localhost
